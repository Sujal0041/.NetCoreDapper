@page
@model Dap.Pages.EditEntryModel
@{
    ViewData["Title"] = "Edit Entry";
}

<h2>Edit Entry</h2>

<form method="post">
    <input type="hidden" asp-for="Entry.Id" />

    <div class="form-group">
        <label asp-for="Entry.FullName"></label>
        <input asp-for="Entry.FullName" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="Entry.Alias"></label>
        <input asp-for="Entry.Alias" class="form-control" />
    </div>

    <!-- Add other fields like IsCorporate, ParentId, etc. similarly -->

    <div class="form-group">
        <button type="submit" class="btn btn-primary">Save Changes</button>
        <a href="/ViewEntries" class="btn btn-secondary">Cancel</a>
    </div>
</form>
